<body>
    <div class="page">
        <div class="header">
            <img src="~/Content/images/logo.gif" alt="logo" />
            <ul>
                <li>@Html.ActionLink("Home", "Index", "Home",null, new { @class="selected"})</li>               
                <li>@Html.ActionLink("Coins Collection", "Index", "CoinsCollections")</li>
                <li>@Html.ActionLink("General coins", "Index", "GeneralCoins")</li>
